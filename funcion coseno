#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;
float funcoseno(float x);

void main()
{
	float x;
	cout<<"Ingrese el valor de x: ";
	cin>>x;
	cout<<"El resultado de la funcion es: "<<funcoseno(x);
	getch();
}
float funcoseno(float x)
{
	float aux;
	aux=3*cos(x) - 1;
	return aux;
}
