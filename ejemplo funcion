#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;
int factorial(int n);

void main()
{
	int N;
	cout<<"Ingrese el valor de N: ";
	cin>>N;
	cout<<"El factorial de numero N es: "<<factorial(N);
	getch();
}
int factorial(int n)
{
	int prod=1;
	for(int i=2;i<=n;i++)
		prod=prod*i
	return prod;
}

